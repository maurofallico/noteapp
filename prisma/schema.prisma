generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas  = ["test"]
}

model Note {
  id       Int      @id @default(autoincrement())
  title    String
  category String[]
  content  String
  status   String

  @@schema("test")
}
